SELECT *
 FROM </home/dsb5/CHAIN/chain-may-2017/chain/code/daisy-spsm/queryRespond/datasets/sepa/waterBodyPressures.n3>
WHERE { ?id <http://data.sepa.org.uk/ont/Water#dataSource> ?dataSource
 ;
 <http://data.sepa.org.uk/ont/Water#identifiedDate> ?identifiedDate ;
 <http://data.sepa.org.uk/ont/Water#affectsGroundwater>
 ?affectsGroundwater ;
  <http://data.sepa.org.uk/ont/Water#waterBodyId> ?waterBodyId
 .}
 

