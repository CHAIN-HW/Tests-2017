# This query should FAIL - no water.n3 file!

SELECT *
 FROM </home/dsb5/CHAIN/chain-may-2017/chain/code/daisy-spsm/queryRespond/datasets/sepa/water.n3>
WHERE { ?id  <http://data.sepa.org.uk/ont/Water#timePeriod> ?timePeriod ;
             <http://www.w3.org/2003/01/ge0/wgs84_pos#geo> ?geo ;
             <http://data.sepa.org.uk/ont/Water#measure> ?measure ;
             <http://data.sepa.org.uk/ont/Water#resource> ?resource 
.}
 

